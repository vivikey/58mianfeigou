    border-right: 0rpx solid #efefef;
    border-bottom: 0rpx solid #efefef;
    border-radius: 0;
}

button[type=primary], button[type=primary][plain] {
    background-color: #50d1fe;
    border-left: 0rpx solid #efefef;
    border-top: 0rpx solid #efefef;
    border-right: 0rpx solid #efefef;
    border-bottom: 0rpx solid #efefef;
    border-radius: 0;
    color: #fff;
    padding: 10rpx 20rpx;
}

button[type=warn], button[type=warn][plain] {
    background-color: #f62004;
