.yusell {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100%;
  transition: all 0.2s linear;
  z-index: 66666;
  overflow: hidden;
}

.yusell.hide {
  height: 0;
}
.yusell .content-box{
	width:100%;
	background: #fff;
	overflow: hidden;
	border-radius: 18rpx;
}
.yusell .content-txt{
	min-height: 250rpx;
	max-height: 750rpx;
	width:100%;
}