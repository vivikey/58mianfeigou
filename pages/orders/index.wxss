button {
  position: relative;
  padding-left: 15rpx;
  padding-right: 15rpx;
  box-sizing: border-box;
  font-size: 14px;
  text-align: center;
  text-decoration: none;
  -webkit-tap-highlight-color: transparent;
  overflow: hidden;
  background: #fff;
  color: #515151;
  box-shadow: 0 3px 1px -2px #777, 0 2px 2px 0 #777, 0 1px 5px 0 #777;
  width: 66.7%;
  height: 60rpx;
  border-radius: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0;
}