@import "/template/storeitem/storeitem.wxss";
@import "/template/mainbar.wxss";
.location{
    width:400rpx;
}
.store_logo{
    width:160rpx;
    height: 140rpx;
    border:1rpx solid #ddd;
    border-radius: 15rpx;
    overflow: hidden;
}
.store_logo image{
    width:100%;
    height: 100%;
}
.store_info{
    height: 140rpx;
    display: flex;
    flex-direction: column;
    justify-content:space-between;
    align-items:flex-start;
}
