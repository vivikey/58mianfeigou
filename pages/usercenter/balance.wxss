.big-top {
  background: #d81e06;
	height: 69px;
  width: 100%;
  color: #fff;
  font-size: 28px;
}

.top-menu {
	width:100%;
	height: 46px;
  background: #d81e06;
  color: #fff;
}
.top-menu .top-menu-kk{
	border:1rpx solid #f2f2f2;
	border-radius: 64rpx;

}
.top-menu-item {
  width: 33%;
  font-size: 12px;
  height: 64rpx;
  display: flex;
  justify-content: center;
	align-items: center;
	border-radius: 64rpx;
	transition: all .3s linear;
}

.top-menu-item.active {
  background: #fff;
  color: #e97f23;
}
